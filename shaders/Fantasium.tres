shader_type canvas_item;

void fragment()
{
	vec4 enchant_texture = vec4(UV, 1.0, 1.0);
	
	enchant_texture.r = sin(enchant_texture.r + enchant_texture.r + TIME);
	enchant_texture.g = sin(enchant_texture.g + enchant_texture.g + TIME);
	enchant_texture.b = cos(enchant_texture.b + enchant_texture.b + TIME);
	
	COLOR = texture(TEXTURE, UV);
	
	if (COLOR.a != 0f) {
		COLOR += enchant_texture * 0.5;
	}
}"

